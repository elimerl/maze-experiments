  "dependencies": {
    "@types/lodash": "^4.14.165",
    "@types/ndarray": "^1.0.8",
    "canvas": "^2.6.1",
    "lodash": "^4.17.20",
    "ndarray": "^1.0.19",
    "open": "^7.3.0",
    "typescript": "4.0.5",
    "zeros": "^1.0.0"
  },
  "devDependencies": {
    "@types/jquery": "^3.5.5",
    "concurrently": "^5.3.0",
    "husky": "^4.3.6",
    "parcel-bundler": "^1.12.4",
    "typedoc": "^0.19.2"
  },
  "scripts": {
    "docs": "typedoc --theme minimal --out docs src && touch docs/.nojekyll",
    "build": "concurrently \"tsc\" \"yarn docs\" \"yarn build:demo\"",
    "demo": "parcel src/demo/index.html",
    "build:demo": "parcel build src/demo/index.html -d docs/demo --public-url .",
    "build:demodocs": "concurrently \"yarn docs\" \"yarn build:demo\""
  }
}